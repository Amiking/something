#!name=NetflixSelect
#!desc=通过panel显示及控制Netflix策略组


[Panel]
NetflixSelect = script-name=NetflixSelect, update-interval=10


[Script]
遍历Netflix策略组 = type=cron,cronexp=2 4 * * *,wake-system=1,timeout=3600,script-path=https://raw.githubusercontent.com/fishingworld/something/main/NetflixSelect/nf_autocheck.js,script-update-interval=0
NetflixChecker = type=cron,cronexp=5 4 * * *,wake-system=1,timeout=3600,script-path=nf_autocheck.js,script-update-interval=0,control-api=1
